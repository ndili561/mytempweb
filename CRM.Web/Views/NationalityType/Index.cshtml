@using CRM.Entity.Model
@using CRM.Entity.Search.Lookup
@model NationalityTypeSearchModel
@{
    ViewData["Title"] = "Nationality Type List";
}
<form asp-action="Grid" method="get">
    <div class="row">
        <div class="panel">
            <div class="panel-body">
                <div class="row">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <div id="filterDiv" class="row">
                                <div class="col-md-12">
                                    <search />
                                </div>
                            </div>
                        </div>
                        <div id="@Model.TargetGridId" class="grid">
                            @Html.Partial("Grid", Model)
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
@Html.Partial("ModalDialog", new ModalDialogViewModel { ParentDivId = "divNationalityTypeParent" })

<script type="text/javascript">

    function displayAddNationalityType() {
        var url = "/NationalityType/Create";
        showDialog("divNationalityTypeParent", "frmNationalityType", url, "Create Nationality");
    }
    function displayEditNationalityType(id) {
        var url = "/NationalityType/Edit?id=" + id;
        showDialog("divNationalityTypeParent", "frmNationalityType", url, "Edit Nationality");
    }
</script>
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}


