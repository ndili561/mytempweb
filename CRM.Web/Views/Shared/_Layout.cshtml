              asp-fallback-href="~/extlibs/jasny-bootstrap/css/jasny-bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />*@

        <!--summernote-->
        <link rel="stylesheet" href="~/extlibs/summernote/summernote.css"
              asp-fallback-href="~/extlibs/summernote/summernote.css"
