@using CRM.Entity.Search.Employee
@model WebApplicationSearchModel
@{
    ViewData["WebApplication"] = "WebApplication";
}
<input type="hidden" asp-for="TargetGridId" />
<div class="ibox-content">
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th><sort display-name="Name" column-name="Name" /></th>
                <th class="text-right"><button type="button" id="btnAddWebApplication" onclick="displayAddWebApplication()" class="btn btn-success btn-labeled fa fa-plus">Add</button></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.WebApplicationSearchResult)
            {
                <tr>
                    <td>
                        <a href="~/WebApplication/Manage?id=@item.Id"> @item.Name</a>
                    </td>
                    <td class="text-right">
                        <a class="btn btn-warning btn-labeled fa fa-edit" href="~/WebApplication/Manage?id=@item.Id"> Manage</a>
                    </td>
                </tr>
            }
        </tbody>
        <tfoot>
            <tr>
                <td class="tc" colspan="12">
                    <pageing />
                </td>
            </tr>
        </tfoot>
    </table>
</div>