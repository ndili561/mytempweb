        <!--Site.css-->
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"
              asp-fallback-href="~/css/site.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
    </environment>

</head>
<body>
    <!-- Skin configuration box -->
    <!-- @Html.Partial("_SkinConfig") -->
    <!-- Wrapper-->
    <div id="wrapper wrapper-content" class="@Html.PageClass()">

        <!-- Navigation -->
        @await Html.PartialAsync("_Navigation")
        <!-- /Navigation -->
        <!-- Page wraper -->
        <div id="page-wrapper" class="gray-bg @ViewBag.SpecialClass">

            <!-- Top Navbar -->
            @await Html.PartialAsync("_TopNavbar")
            <!-- /Top Navbar -->

            <div class="wrapper wrapper-content">
                <!-- Main view -->
                @Html.AntiForgeryToken()
                @RenderBody()
                <div id="ajaxInProgress" class="sk-spinner sk-spinner-wave" style="display: none">
                    <div class="sk-rect1"></div>
                    <div class="sk-rect2"></div>
                    <div class="sk-rect3"></div>
                    <div class="sk-rect4"></div>
                    <div class="sk-rect5"></div>
                </div>
                @Html.Partial("_Footer")
            </div>
            @await Html.PartialAsync("_RightSidebar")

        </div>
    </div>
    <!-- End page wrapper-->



